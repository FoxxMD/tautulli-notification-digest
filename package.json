{
  "name": "tautulli-notification-digest",
  "version": "0.0.1",
  "description": "Consolidate tautulli notifications",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/FoxxMD/tautulli-notification-digest"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.1.0"
  },
  "author": "FoxxMD",
  "license": "MIT",
  "private": false,
  "exports": {
    ".": {
      "import": "./src/index.js"
    }
  },
  "scripts": {
    "test": "nyc ./node_modules/.bin/_mocha 'tests/**/*.test.ts'",
    "build": "tsc",
    "start": "node src/index.js",
    "schema": "yarn run -s schema-operator",
    "schema-operator": "typescript-json-schema tsconfig.json OperatorJsonConfig --out src/common/schema/operator.json --required --tsNodeRegister --refs --validationKeywords deprecationMessage",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@awaitjs/express": "^0.6.3",
    "@foxxmd/redact-string": "^0.1.2",
    "@foxxmd/winston": "^3.3.31",
    "@supercharge/promise-pool": "^3.0.0",
    "ajv": "^7.2.4",
    "body-parser": "^1.19.0",
    "common-tags": "^1.8.2",
    "concat-stream": "^2.0.0",
    "croner": "^6.0.7",
    "dayjs": "^1.11.8",
    "deepmerge": "^4.3.1",
    "discord.js": "14.11.0",
    "dotenv": "^16.3.1",
    "es6-error": "^4.1.1",
    "express": "^4.17.1",
    "formidable": "^2.1",
    "got": "^11.8.2",
    "pony-cause": "^1.1.1",
    "sequelize": "^6.32.1",
    "sqlite3": "^5.1.6",
    "toad-scheduler": "^3.0.0",
    "triple-beam": "^1.3.0",
    "umzug": "^3.2.1",
    "winston-daily-rotate-file": "^4.5.0",
    "winston-duplex": "0.1.3",
    "winston-null": "^2.0.0",
    "winston-transport": "^4.4.0",
    "yaml": "^2.3.1"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@tsconfig/node18": "^1.0.1",
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.5",
    "@types/concat-stream": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/formidable": "^2.0.5",
    "@types/mocha": "^9.1.0",
    "@types/node": "^18.0.0",
    "@types/triple-beam": "^1.3.2",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "lorem-ipsum": "^2.0.8",
    "mocha": "^9.2.1",
    "nyc": "^15.1.0",
    "patch-package": "^8.0.0",
    "sequelize-cli": "^6.6.1",
    "source-map-support": "^0.5.21",
    "ts-essentials": "^9.1.2",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.13.0",
    "typescript": "^4.9.5",
    "typescript-json-schema": "~0.55"
  }
}
